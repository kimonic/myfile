<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/global_background_color"
    >

    <com.tudoujf.ui.MTopBarView
        android:id="@+id/mtb_act_web"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@color/act_register_tvbac1"
        app:centerText="@string/act_web_title"
        app:leftImage="@drawable/xvector_back" />
    <WebView
        android:id="@+id/wv_act_web"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>